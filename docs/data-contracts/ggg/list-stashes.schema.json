{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Stashes",
  "title": "List Stashes response",
  "description": "Reference: https://www.pathofexile.com/developer/docs/reference#stashes-list",
  "definitions": {
	"Stashes": {
	  "type": "object",
	  "additionalProperties": false,
	  "properties": {
		"stashes": {
		  "type": "array",
		  "items": {
			"$ref": "#/definitions/Stash"
		  }
		}
	  },
	  "required": [
		"stashes"
	  ],
	  "title": "Stashes"
	},
	"Stash": {
	  "type": "object",
	  "additionalProperties": false,
	  "properties": {
		"id": {
		  "type": "string"
		},
		"parent": {
		  "type": "string"
		},
		"name": {
		  "type": "string"
		},
		"type": {
		  "$ref": "#/definitions/Type"
		},
		"index": {
		  "type": "integer"
		},
		"metadata": {
		  "$ref": "#/definitions/Metadata"
		}
	  },
	  "required": [
		"id",
		"index",
		"metadata",
		"name",
		"type"
	  ],
	  "title": "Stash"
	},
	"Metadata": {
	  "type": "object",
	  "additionalProperties": false,
	  "properties": {
		"public": {
		  "type": "boolean",
		  "description": "true if present"
		},
		"folder": {
		  "type": "boolean",
		  "description": "true if present"
		},
		"colour": {
		  "type": "string"
		},
		"map": {
		  "$ref": "#/definitions/Map"
		}
	  },
	  "required": [
		"colour"
	  ]
	},
	"Map": {
	  "type": "object",
	  "additionalProperties": false,
	  "properties": {
		"series": {
		  "type": "integer"
		}
	  },
	  "required": [
		"series"
	  ],
	  "title": "Map"
	},
	"Type": {
	  "type": "string",
	  "enum": [
		"CurrencyStash",
		"NormalStash",
		"QuadStash",
		"PremiumStash",
		"MapStash",
		"FragmentStash"
	  ],
	  "title": "Type"
	}
  }
}
